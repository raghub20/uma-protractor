{
  "name": "mosaic",
  "version": "0.0.0",
  "config": {
    "BaseHref": "/",
    "DeployUrl": "http://localhost:12345/",
    "ComputerName": "localhost",
    "IISSiteName": "mosaic",
    "IISAppName": "mosaic"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:developer": "node generate-config developer && cross-env-shell ng build -c developer && ng serve --port 4202",
    "start:developer:mocked": "cross-env ng e2e --port 4202 --protractor-config e2e/protractor.mocked-serve.conf.js",
    "start:prod-local": "npm run build:prod-local && http-server ./dist/mosaic -g -a localhost",
    "start:server": "cross-env-shell AABB=$(pwd) && docker run -it --rm -p 8080:80 --volume ${AABB}/dist/mosaic:/app/wwwroot cartus_client_services",
    "start:server-default": "cross-env-shell AABB=$(pwd) && docker run -it --rm -p 8080:80 cartus_client_services",
    "build": "ng build",
    "build:static": "ng build -c static",
    "build:prod-local": "ng build --prod --stats-json --source-map",
    "build:prod:win": "cross-env-shell ng build --aot --prod --stats-json",
    "build:server-dev": "ng build --stats-json --watch --aot --build-optimizer",
    "build:stats": "ng build --prod --stats-json --source-map",
    "build:mocked": "ng build -c mocked",
    "build:developer": "node generate-config developer && cross-env-shell ng build -c developer",
    "build:dv": "node generate-config dv && cross-env-shell ng build -c dv",
    "postbuild:dv": "cross-env-shell node ./add_build_number.js dv && node deploy/iis/build_package.js Clients portal dv",
    "build:dev": "node generate-config dev && cross-env-shell ng build -c dev",
    "postbuild:dev": "cross-env-shell node ./add_build_number.js dev",
    "build:ft": "node generate-config ft && cross-env-shell ng build -c ft",
    "postbuild:ft": "cross-env-shell node ./add_build_number.js ft",
    "build:qa": "node generate-config qa && cross-env-shell ng build -c qa",
    "postbuild:qa": "cross-env-shell node ./add_build_number.js qa",
    "build:prod": "node generate-config production && cross-env-shell ng build -c production",
    "postbuild:prod": "cross-env-shell node ./add_build_number.js prod",
    "build:integration": "node generate-config dv && cross-env-shell ng build -c dv",
    "postbuild:integration": "cross-env-shell node deploy/iis/build_package.js $npm_package_config_IISSiteName $npm_package_config_IISAppName",
    "build:iis": "cross-env-shell ng build --aot --prod --stats-json --base-href $npm_package_config_BaseHref --deploy-url $npm_package_config_DeployUrl",
    "postbuild:iis": "cross-env-shell node deploy/iis/build_package.js $npm_package_config_IISSiteName $npm_package_config_IISAppName",
    "build:iis:developer": "cross-env-shell ng build -c developer --aot --prod --stats-json --base-href $npm_package_config_BaseHref --deploy-url $npm_package_config_DeployUrl",
    "postbuild:iis:developer": "cross-env-shell node deploy/iis/build_package.js $npm_package_config_IISSiteName $npm_package_config_IISAppName",
    "test": "ng test --watch=false",
    "test:coverage": "ng test -c code-coverage",
    "posttest:coverage": "cd reports/coverage/html && cross-env mkdir -p vsts && inline-css -i index.html -o vsts/index.html",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:mocked": "cross-env ng e2e --port 4202 --protractor-config e2e/protractor.mocked.conf.js",
    "e2e:mocked-multi": "cross-env ng e2e --port 4202 --protractor-config e2e/protractor.mocked-multi.conf.js",
    "e2e:mocked-wip": "cross-env CUCUMBER_TAGS='@wip' ng e2e --port 4202 --protractor-config e2e/protractor.mocked-wip.conf.js",
    "e2e:integration": "ng e2e --port 4202 -c integration",
    "pree2e:ie": "cross-env cd node_modules/protractor/bin/webdriver-manager  && webdriver-manager --ie32 --chrome false --gecko false --verbose update",
    "e2e:ie": "cross-env ng e2e --port 4202 -c ie",
    "e2e:browserstack": "cross-env ng e2e --port 4202 --protractor-config e2e/protractor.browserstack.js",
    "e2e:browserstack-multi": "cross-env ng e2e --port 4202 --protractor-config e2e/protractor.browserstack-multi.js",
    "e2e:browserstack:mocked": "cross-env ng e2e --port 4202 --protractor-config e2e/protractor.browserstack.mocked.js",
    "e2e:browserstack-multi:mocked": "cross-env ng e2e --port 4202 --protractor-config e2e/protractor.browserstack-multi.mocked.js",
    "e2e:setup": "./node_modules/protractor/bin/webdriver-manager  --gecko false --verbose update",
    "analyzer": "webpack-bundle-analyzer dist/mosaic/stats.json",
    "dump:iis": "cross-env msdeploy --verb sync --allowUntrusted true --source iisApp=$npm_package_config_IISSiteName/$npm_package_config_IISAppName,ComputerName=$npm_package_config_ComputerName --dest package=sample.zip",
    "deploy:iis": "cross-env msdeploy --verb sync --allowUntrusted true --source package=dist/iis/$npm_package_config_IISAppName.zip --dest iisApp=$npm_package_config_IISSiteName/$npm_package_config_IISAppName,ComputerName=$npm_package_config_ComputerName",
    "sonar": "sonar-scanner -X",
    "compodoc": "npx compodoc -p src/tsconfig.app.json",
    "webdriver-update": "webdriver-manager update"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.6",
    "@angular/cdk": "^7.3.4",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.3.3",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@types/lodash": "^4.14.136",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "stacktrace-js": "^2.0.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.3",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@compodoc/compodoc": "^1.1.9",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/cors": "^2.8.4",
    "@types/cucumber": "^4.0.5",
    "@types/express": "^4.16.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/mongoose": "^5.3.27",
    "@types/node": "~8.9.4",
    "@types/puppeteer": "^1.12.2",
    "@types/selenium-webdriver": "^3.0.15",
    "browserstack-local": "^1.3.7",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codelyzer": "~4.5.0",
    "cross-env": "^5.2.0",
    "cucumber": "^5.1.0",
    "cucumber-html-report": "^0.6.5",
    "cucumber-junit": "^1.7.1",
    "express": "^4.16.4",
    "inline-css-cli": "0.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "mongodb-memory-server": "^4.2.2",
    "mongoose": "^5.5.5",
    "ng-swagger-gen": "^1.5.0",
    "protractor": "~5.4.0",
    "protractor-cucumber-framework": "^6.1.1",
    "protractor-multiple-cucumber-html-reporter-plugin": "^1.8.0",
    "puppeteer": "^1.13.0",
    "rxjs-marbles": "^5.0.0",
    "rxjs-tslint-rules": "^4.18.2",
    "sonar-scanner": "^3.1.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.2.2",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
